{
    "workbench.colorTheme": "Jetbrains default theme",
    "editor.minimap.enabled": false,
    "files.trimTrailingWhitespace": true,
    "explorer.sortOrder": "type",
    "workbench.iconTheme": "webstorm-icons",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#000",
        "terminal.background": "#fff",
        "activityBar.background": "#ddd",
        "activityBar.border": "#aaa",
        "activityBarBadge.background": "#f33",
        "activityBar.foreground": "#666",
        "statusBar.background": "#666",
        "panel.background": "#eee",
        "panelTitle.activeForeground": "#000",
        "panelTitle.inactiveForeground": "#999",
        "panelTitle.activeBorder": "#666",
        "foreground": "#444",
        "tab.inactiveBackground": "#ddd",
    },
    "workbench.fontAliasing": "antialiased",
    "terminal.integrated.scrollback": 10000,
    "editor.fontWeight": "600",
    "editor.cursorBlinking": "smooth",
    "problems.decorations.enabled": true,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.tabSizing": "shrink",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "shortcuts.buttons": [
        "terminal , workbench.action.terminal.toggleTerminal , Toggle terminal panel",
        "bug , workbench.view.debug , View debug panel",
        "tools , workbench.view.extensions , Manage Extenstions",
        "search , workbench.view.search , Search"
    ],
    "git.autofetch": true,
    "workbench.statusBar.feedback.visible": false
}
